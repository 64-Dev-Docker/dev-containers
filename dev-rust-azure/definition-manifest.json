{
	"definitionVersion": "0.200.0",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"rust:${VERSION}-1"
		]
	},
	"dependencies": {
		"image": "rust:1",
		"imageLink": "https://hub.docker.com/_/rust",
		"git": {
			"Oh My Bash!": "/root/.oh-my-bash",
			"Oh My Zsh!": "/root/.oh-my-zsh"
		},
		"cargo": {
			"rls": "rls --version",
			"rustfmt": "rustfmt --version",
			"rust-analysis": null,
			"rust-src": null,
			"clippy": null
		}
	}
}
